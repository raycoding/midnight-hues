<h1>Login</h1>
<%= form_for @user_session do |f| %>
  <%= f.label :primary_phone %><br />
  <%= f.text_field :primary_phone %><br />
  <br />
  <%= f.label :password %><br />
  <%= f.password_field :password %><br />
  <br />
  <%= f.submit "Login" %>
<% end %>
<a href=<%= new_user_path %>>Sign Up!</a>